{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "AllowedHosts": "*",

  "CookieOptions": {
    "Name": "ObsSecurtiyCookie",
    "LoginPath": "/Auth/Login",
    "LogOutPath": "/Auth/LogOut",
    "AccessDeniedPath": "/Auth/AccessDenied",
    "SlidingExpiration": true,
    "TimeOut":120
  }
}
